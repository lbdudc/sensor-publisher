    "meow": "^12.0.1",
    "openai": "^4.32.1",
    "@lbdudc/sensor-dsl": "^0.1.14",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "spl-js-engine": "^4.0.4",
    "vue": "^3.2.41",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@mdi/font": "^7.1.96",
    "@mdi/js": "^7.0.96",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vuetify": "^2.1.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jszip": "^3.10.1",
    "lint-staged": "^13.2.2",
    "material-design-icons-iconfont": "^6.7.0",
    "monaco-editor": "^0.34.1",
    "monaco-editor-vue3": "^0.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.2.4",
    "vite": "^5.0.11",
    "vite-plugin-vuetify": "2.0.1"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{json,md,yml}": "prettier --write",
    "*.{js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "name": "sensorbuilder",
  "scripts": {
    "build": "cd src/client && vite build",
    "deploy_prod": "npm run build && npm run serve",
    "dev_client": "cd src/client && vite build",
    "dev": "concurrently \"npm run dev_client\" \"npm run serve\"",
    "deploy": "node src/server/deploy.js",
    "lint_eslint": "eslint --ext js,vue --fix src",
    "lint_prettier": "prettier --write --ignore-unknown src",
    "lint": "npm-run-all lint_eslint lint_prettier",
    "prepare": "husky install",
    "preview": "cd src/client && vite preview",
    "serve": "node src/server/server.js"
  },
  "type": "module",
  "version": "0.2.0"
}
