{
  "bin": {
    "sensorbuilder": "src/cli.js"
  },
  "dependencies": {
    "@babel/register": "^7.16.9",
    "autoprefixer": "^9.8.8",
    "code-uploader": "git+https://gitlab.lbd.org.es/GEMA/lps/gispublisher/code-uploader.git",
    "file-saver": "^2.0.2",
    "jointjs": "^3.5.1",
    "jszip": "^3.5.0",
    "meow": "^12.0.1",
    "normalize.css": "^8.0.1",
    "postcss": "^7.0.39",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.2",
    "sensor-dsl": "git+https://gitlab.lbd.org.es/pcmg/sensor-dsl.git",
    "spl-js-engine": "^3.1.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "vue": "^2.7.15",
    "vue-i18n": "^8.28.2",
    "vue-router": "^3.6.5",
    "vuetify": "^2.6.15"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-eslint": "^4.5.19",
    "@vue/cli-plugin-router": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "monaco-editor": "^0.24.0",
    "monaco-editor-webpack-plugin": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "~1.32.12",
    "sass-loader": "^10.1.0",
    "svg-inline-loader": "^0.8.2",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-template-compiler": "^2.7.14",
    "vuetify-loader": "^1.9.2"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "globals": {
      "L": false,
      "MV": false
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "root": true,
    "rules": {
      "no-console": 0,
      "vue/component-name-in-template-casing": [
        "error",
        "kebab-case",
        {
          "registeredComponentsOnly": false
        }
      ]
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{css,html,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "tailwindcss": {}
    }
  },
  "name": "sensorbuilder",
  "scripts": {
    "debug": "npm-run-all -p -r buildwatch",
    "serve": "vue-cli-service serve -p --mode development",
    "buildwatch": "vue-cli-service build --watch --no-clean --mode development",
    "build": "vue-cli-service build",
    "lint": "npm-run-all lint_eslint lint_prettier",
    "lint_eslint": "eslint --ext js,vue --fix src",
    "lint_prettier": "prettier --write --ignore-unknown src",
    "prepare": "husky install"
  },
  "version": "0.2.0"
}
